label>Confirm Password</label>\n                        <input type=\"password\" onChange={(e) => setCPassword(e.target.value)} />\n                    </div>\n                    <div>\n                        <button >Signup</button>\n                    </div>\n                </form>\n            </div>\n            <Link to=\"/Login\">Already A User?</Link>\n        </>\n    );\n}\n\nexport default SignUp;","/Users/justinezaver/Desktop/NewTreasuresFolder/WorkingOn/treasures-web-frontend/src/pages/blogs/blogComp.js",["117"],"/Users/justinezaver/Desktop/NewTreasuresFolder/WorkingOn/treasures-web-frontend/src/index.js",["118"],{"ruleId":"119","severity":1,"message":"120","line":2,"column":27,"nodeType":"121","messageId":"122","endLine":2,"endColumn":33},{"ruleId":"119","severity":1,"message":"123","line":2,"column":35,"nodeType":"121","messageId":"122","endLine":2,"endColumn":40},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"119","severity":1,"message":"128","line":29,"column":15,"nodeType":"121","messageId":"122","endLine":29,"endColumn":19},{"ruleId":"124","replacedBy":"129"},{"ruleId":"126","replacedBy":"130"},{"ruleId":"119","severity":1,"message":"120","line":4,"column":27,"nodeType":"121","messageId":"122","endLine":4,"endColumn":33},{"ruleId":"119","severity":1,"message":"123","line":4,"column":35,"nodeType":"121","messageId":"122","endLine":4,"endColumn":40},{"ruleId":"119","severity":1,"message":"120","line":4,"column":27,"nodeType":"121","messageId":"122","endLine":4,"endColumn":33},{"ruleId":"119","severity":1,"message":"123","line":4,"column":35,"nodeType":"121","messageId":"122","endLine":4,"endColumn":40},{"ruleId":"119","severity":1,"message":"131","line":4,"column":8,"nodeType":"121","messageId":"122","endLine":4,"endColumn":23},{"ruleId":"119","severity":1,"message":"132","line":5,"column":8,"nodeType":"121","messageId":"122","endLine":5,"endColumn":22},{"ruleId":"119","severity":1,"message":"133","line":6,"col